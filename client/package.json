{
  "name": "client",
  "description": "Shift Dashboard Frontend - React TypeScript SPA with MUI",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "start": "vite preview",
    "preview": "vite preview",
    "test": "vitest --passWithNoTests",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage --passWithNoTests",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "clean": "rm -rf dist coverage playwright-report test-results"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
